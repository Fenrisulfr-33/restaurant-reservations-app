ut/ErrorAlert\";\r\nimport ReservationList from \"./ReservationList\";\r\nimport TablesList from \"./TablesList\";\r\n\r\n/**\r\n * Defines the dashboard page.\r\n * @param date\r\n *  the date for which the user wants to view reservations.\r\n * @returns {JSX.Element}\r\n */\r\nfunction Dashboard({ date }) {\r\n  const [reservations, setReservations] = useState([]);\r\n  const [reservationsError, setReservationsError] = useState(null);\r\n  const [tables, setTables] = useState([]);\r\n  const [tablesError, setTablesError] = useState(null);\r\n  // load the dashboard\r\n  useEffect(loadDashboard, [date]);\r\n\r\n  // get the list of reservations for the day and tables list\r\n  function loadDashboard() {\r\n    const ac = new AbortController();\r\n    setReservationsError(null);\r\n    listReservations({ date }, ac.signal)\r\n      .then(setReservations)\r\n      .catch(setReservationsError);\r\n    listTables()\r\n      .then(setTables);\r\n    return () => ac.abort();\r\n  }\r\n\r\n  useEffect(loadTables, [setTables]);\r\n\r\n  function loadTables() {\r\n    const ac = new AbortController();\r\n    setTablesError(null);\r\n      listTables()\r\n        .then(setTables)\r\n        .catch(setTablesError);\r\n    return () => ac.abort();\r\n  }\r\n\r\n  function finish(table_id) {\r\n    const ac = new AbortController();\r\n    deleteReservation(table_id, ac.signal)\r\n    .then(loadDashboard)\r\n    .catch(setReservationsError);\r\n  }\r\n\r\n  return (\r\n    <main>\r\n      <h1>Dashboard</h1>\r\n      <div className=\"d-md-flex mb-3\">\r\n        <h4 className=\"mb-0\">Reservations for date</h4>\r\n      </div>\r\n      <ErrorAlert error={reservationsError} />\r\n      <nav>\r\n        <Link to={`/dashboard?date=${previous(date)}`}>\r\n          <button>Previous</button>\r\n        </Link>\r\n        <Link to={`/dashboard?date=${today()}`}>\r\n          <button>Today</button>\r\n        </Link>\r\n        <Link to={`/dashboard?date=${next(date)}`}>\r\n          <button>Next</button>\r\n        </Link>\r\n      </nav>\r\n      <ReservationList reservations={reservations} date={date}/>\r\n      <TablesList tables={tables} finish={finish} />\r\n    </main>\r\n  );\r\n}\r\n\r\nexport default Dashboard;\r\n","C:\\Users\\Gin\\Documents\\code\\thinkful\\final-capstone\\restaurant-reservations-app\\front-end\\src\\utils\\date-time.js",[],"C:\\Users\\Gin\\Documents\\code\\thinkful\\final-capstone\\restaurant-reservations-app\\front-end\\src\\utils\\api.js",[],"C:\\Users\\Gin\\Documents\\code\\thinkful\\final-capstone\\restaurant-reservations-app\\front-end\\src\\layout\\ErrorAlert.js",[],"C:\\Users\\Gin\\Documents\\code\\thinkful\\final-capstone\\restaurant-reservations-app\\front-end\\src\\utils\\format-reservation-date.js",[],"C:\\Users\\Gin\\Documents\\code\\thinkful\\final-capstone\\restaurant-reservations-app\\front-end\\src\\reservations\\Reservations.js",[],"C:\\Users\\Gin\\Documents\\code\\thinkful\\final-capstone\\restaurant-reservations-app\\front-end\\src\\utils\\useQuery.js",[],"C:\\Users\\Gin\\Documents\\code\\thinkful\\final-capstone\\restaurant-reservations-app\\front-end\\src\\dashboard\\ReservationList.js",[],"C:\\Users\\Gin\\Documents\\code\\thinkful\\final-capstone\\restaurant-reservations-app\\front-end\\src\\dashboard\\Reservation.js",[],"C:\\Users\\Gin\\Documents\\code\\thinkful\\final-capstone\\restaurant-reservations-app\\front-end\\src\\tables\\Tables.js",[],"C:\\Users\\Gin\\Documents\\code\\thinkful\\final-capstone\\restaurant-reservations-app\\front-end\\src\\dashboard\\TablesList.js",[],"C:\\Users\\Gin\\Documents\\code\\thinkful\\final-capstone\\restaurant-reservations-app\\front-end\\src\\dashboard\\Table.js",["88","89"],"import { deleteReservation, listTables } from \"../utils/api\";\r\nimport { useHistory } from 'react-router-dom';\r\n\r\n/**\r\n * \r\n * @param {reservation}\r\n *  a reservation object \r\n * @returns \r\n *  a row with the reservation data\r\n */\r\n function Table({ table, finish }) 