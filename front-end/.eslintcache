import { today } from \"../utils/date-time\";\r\nimport useQuery from '../utils/useQuery';\r\nimport Tables from \"../tables/Tables\";\r\nimport SeatForm from \"../seats/SeatForm\";\r\nimport SearchPage from \"../search/SearchPage\";\r\n\r\n/**\r\n * Defines all the routes for the application.\r\n *\r\n * You will need to make changes to this file.\r\n *\r\n * @returns {JSX.Element}\r\n */\r\nfunction Routes() {\r\n  const query = useQuery();\r\n  const date = query.get('date');\r\n  const mobile = query.get('mobile_number');\r\n\r\n  return (\r\n    <Switch>\r\n      <Route exact={true} path=\"/\">\r\n        <Redirect to={\"/dashboard\"} />\r\n      </Route>\r\n      <Route path=\"/dashboard\">\r\n        <Dashboard date={date || today()} />\r\n      </Route>\r\n      <Route path=\"/tables/new\">\r\n        <Tables />\r\n      </Route>\r\n      <Route path=\"/reservations/new\">\r\n        <Reservations />\r\n      </Route>\r\n      <Route path='/reservations/:reservation_id/seat'>\r\n        <SeatForm />\r\n      </Route>\r\n      <Route path='/search'>\r\n        <SearchPage />\r\n      </Route>\r\n      <Route>\r\n        <NotFound />\r\n      </Route>\r\n    </Switch>\r\n  );\r\n}\r\n\r\nexport default Routes;\r\n","C:\\Users\\Gin\\Documents\\code\\thinkful\\final-capstone\\restaurant-reservations-app\\front-end\\src\\layout\\NotFound.js",[],"C:\\Users\\Gin\\Documents\\code\\thinkful\\final-capstone\\restaurant-reservations-app\\front-end\\src\\dashboard\\Dashboard.js",["92"],"import React, { useEffect, useState } from \"react\";\r\nimport { Link } from 'react-router-dom';\r\nimport { listReservations, listTables, freeTable, updateReservation } from \"../utils/api\";\r\nimport { next, previous, today } from '../utils/date-time'\r\nimport ErrorAlert from \"../layout/ErrorAlert\";\r\nimport ReservationList from \"./ReservationList\";\r\nimport TablesList from \"./TablesList\";\r\n\r\n/**\r\n * Defines the dashboard page.\r\n * @param date\r\n *  the date for which the user wants to view reservations.\r\n * @returns {JSX.Element}\r\n */\r\nfunction Dashboard({ date }) {\r\n  const [reservations, setReservations] = useState([]);\r\n  const [reservationsError, setReservationsError] = useState(null);\r\n  const [tables, setTables] = useState([]);\r\n  const [tablesError, setTablesError] = useState(null);\r\n  // load the dashboard\r\n  useEffect(loadDashboard, [date]);\r\n\r\n  // get the list of reservations for the day and tables list\r\n  function loadDashboard() {\r\n    const ac = new AbortController();\r\n    setReservationsError(null);\r\n    listReservations({ date }, ac.signal)\r\n      .then(setReservations)\r\n      .catch(setReservationsError);\r\n    listTables().then(setTables);\r\n    return () => ac.abort();\r\n  }\r\n\r\n  useEffect(loadTables, []);\r\n\r\n  function loadTables() {\r\n    const ac = new AbortController();\r\n    setTablesError(null);\r\n      listTables()\r\n        .then(setTables)\r\n        .catch(setTablesError);\r\n    return () => ac.abort();\r\n  }\r\n\r\n  function finish(table) {\r\n    const ac = new AbortController();\r\n    freeTable(table.table_id, ac.signal)\r\n    .then(loadDashboard)\r\n    // .then(loadTables)\r\n    .catch(setReservationsError);\r\n  }\r\n\r\n  return (\r\n    <main>\r\n      <h1>Dashboard</h1>\r\n      <div className=\"d-md-flex mb-3\">\r\n        <h4 className=\"mb-0\">Reservations for date</h4>\r\n      </div>\r\n      <ErrorAlert error={reservationsError} />\r\n      <ErrorAlert error={tablesError} />\r\n      <nav>\r\n        <Link to={`/dashboard?date=${previous(date)}`}>\r\n          <button>Previous</button>\r\n        </Link>\r\n        <Link to={`/dashboard?date=${today()}`}>\r\n          <button>Today</button>\r\n        </Link>\r\n        <Link to={`/dashboard?date=${next(date)}`}>\r\n          <button>Next</button>\r\n        </Link>\r\n      </nav>\r\n      <ReservationList reservations={reservations} date={date}/>\r\n      <TablesList tables={tables} finish={finish} />\r\n    </main>\r\n  );\r\n}\r\n\r\nexport default Dashboard;\r\n","C:\\Users\\Gin\\Documents\\code\\thinkful\\final-capstone\\restaurant-reservations-app\\front-end\\src\\utils\\date-time.js",[],"C:\\Users\\Gin\\Documents\\code\\thinkful\\final-capstone\\restaurant-reservations-app\\front-end\\src\\utils\\api.js",[],"C:\\Users\\Gin\\Documents\\code\\thinkful\\final-capstone\\restaurant-reservations-app\\front-end\\src\\layout\\ErrorAlert.js",[],"C:\\Users\\Gin\\Documents\\code\\thinkful\\final-capstone\\restaurant-reservations-app\\front-end\\src\\utils\\format-reservation-date.js",[],"C:\\Users\\Gin\\Documents\\code\\thinkful\\final-capstone\\restaurant-reservations-app\\front-end\\src\\reservations\\Reservations.js",[],"C:\\Users\\Gin\\Documents\\code\\thinkful\\final-capstone\\restaurant-reservations-app\\front-end\\src\\utils\\useQuery.js",[],"C:\\Users\\Gin\\Documents\\code\\thinkful\\final-capstone\\restaurant-reservations-app\\front-end\\src\\dashboard\\ReservationList.js",[],"C:\\Users\\Gin\\Documents\\code\\thinkful\\final-capstone\\restaurant-reservations-app\\front-end\\src\\dashboard\\Reservation.js",[],"C:\\Users\\Gin\\Documents\\code\\thinkful\\final-capstone\\restaurant-reservations-app\\front-end\\src\\tables\\Tables.js",[],"C:\\Users\\Gin\\Documents\\code\\thinkful\\final-capstone\\restaurant-reservations-app\\front-end\\src\\dashboard\\TablesList.js",[],"C:\\Users\\Gin\\Documents\\code\\thinkful\\final-capstone\\restaurant-reservations-app\\front-end\\src\\dashboard\\Table.js",[],"C:\\Users\\Gin\\Documents\\code\\thinkful\\final-capstone\\restaurant-reservations-app\\front-end\\src\\seats\\SeatForm.js",[],"C:\\Users\\Gin\\Documents\\code\\thinkful\\final-capstone\\restaurant-reservations-app\\front-end\\src\\seats\\TablesDropDown.js",[],"C:\\Users\\Gin\\Documents\\code\\thinkful\\final-capstone\\restaurant-reservations-app\\front-end\\src\\search\\SearchPage.js",[],{"ruleId":"93","replacedBy":"94"},{"ruleId":"95","replaced